#!/bin/bash

archive_mimes=(
    application/x-compressed-tar
    application/x-rar
    application/zip
)

if test "$(xdg-mime query default $(xdg-mime query filetype "$1"))" = file-roller.desktop; then
    dir=./"$(basename "$1")".unzip.$$
    mkdir -p "$dir"
    file-roller -e "$dir" "$1"
    up "$dir"
    exit
fi
of "$1";
